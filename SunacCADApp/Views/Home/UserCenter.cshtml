@using SunacCADApp.Entity
@using SunacCADApp.Data
@{
    IList<Sys_Model> Model = ViewBag.models;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CAD用户中心</title>
    <link href="~/Content/Css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Css/site.css" rel="stylesheet" />
    <script src="http://libs.baidu.com/jquery/2.0.0/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/Plugins/layer/layer.js" type="text/javascript"></script>
    <script src="~/Content/Plugins/laydate/laydate.js" type="text/javascript"></script>
</head>
<body>
    <div id="header">
        <div class="header-left">
            设计协同办公平台
        </div>
    </div>
    <div id="wrapper">
        <div class="sidebar_left">
            <dl>
                <dt class="down">
                    <a class="goyeer-icon  goyeer-icon-standard-part">标准部品库</a>
                </dt>
                <dd>
                    <a href="">外窗</a>
                    <a href="">门</a>
                    <a href="">栏杆</a>
                </dd>
            </dl>
            <dl>
                <dt class="down">
                    <a class="goyeer-icon  goyeer-icon-space">标准空间模块库</a>
                </dt>
                <dd>
                    <a href="">厨房</a>
                    <a href="">卫生间</a>
                    <a href="">空调</a>
                </dd>
            </dl>
            <dl>
                <dt>
                    <a class="goyeer-icon  goyeer-icon-space">项目管理</a>
                </dt>
            </dl>
            <dl>
                <dt class="down">
                    <a class="goyeer-icon  goyeer-icon-space">后台管理</a>
                </dt>
                <dd>
                    <a href="/SysUser" class="selected" target="myiframe">用户管理</a>
                    <a href="/SysRole" target="myiframe">角色管理</a>
                    <a href="/ArgumentSetting" target="myiframe">配置管理</a>
                    <a href="/CompanyInfo" target="myiframe">机构管理</a>
                    <a href="#">日志管理</a>
                </dd>
            </dl>
        </div> 
        <div class="sidebar_main">
            <div id="main-frame">
                  <iframe width="100%" height="100%" name="myiframe" frameborder="0">

                  </iframe>
            </div>
        </div>
    </div>
</body>
</html>
<script type="text/javascript">
    $(function () {
        $("dl dd").on("click", "a", function () {
            $("dl dd a").removeClass("selected");
            $(this).addClass("selected");
            
        })
    })
</script>
